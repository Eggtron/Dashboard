doctype html
html(lang="de")
head
  meta(charset="utf-8")
  meta(http-equiv="X-UA-Compatible")
  meta(name="viewport")(content="width=device-width, initial-scale=1, shrink-to-fit=no")
  
  title SB Admin 2 - Dashboard

  link(href="/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css")
  link(href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet")

  link(href="/stylesheets/sb-admin-2.min.css" rel="stylesheet")

body#page-top

  div#wrapper 

    ul.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion#accordionSidebar

      a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='index')
        div.sidebar-brand-icon.rotate-n-15
          i.fas.fa-laugh-wink
        div.sidebar-brand-text.mx-3 SB Admin 
        sup 2

      hr.sidebar-divider.my-0

      li.nav-item.active
        a.nav-link(href='index')
          i.fas.fa-fw.fa-tachometer-alt
            span Dashboard

      hr.sidebar-divider

      div.sidebar-heading Interface

      li.nav-item
        a.nav-link.collapsed(href="#" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo")
          i.fas.fa-fw.fa-cog
          span Components
        div#collapseTwo.collapse(aria-labelledby="headingTwo" data-parent="#accordionSidebar")
          div.bg-white.py-2.collapse-inner.rounded
            h6.collapse-header Custom Components
            a.collapse-item(href='buttons') Buttons
            a.collapse-item(href='cards') Cards
      
      li.nav-item
        a.nav-link.collapsed(href="#" data-toggle="collapse" data-target="#collapseUtilities" aria-expanded="true" aria-controls="collapseUtilities")
          i.fas.fa-fw.fa-wrench
          span Utilities
        div#collapseUtilities.collapse(aria-labelledby="headingUtilities" data-parent="#accordionSidebar")
          div.bg-white.py-2.collapse-inner.rounded
            h6.collapse-header Custom Utilities
            a.collapse-item(href='utilities-color') Colors
            a.collapse-item(href='utilities-border') Borders
            a.collapse-item(href='utilities-animation') Animations
            a.collapse-item(href='utilities-other') Other

      hr.sidebar-divider

      div.sidebar-heading Addons

      li.nav-item
        a.nav-link.collapsed(href="#" data-toggle="collapse" data-target="#collapsePages" aria-expanded="true" aria-controls="collapsePages")
          i.fas.fa-fw.fa-folder
            span Pages
        div#collapsePages.collapse(aria-labelledby="headingPages" data-parent="#accordionSidebar")
          div.bg-white.py-2.collapse-inner.rounded
            h6.collapse-header Login Screen
            a.collapse-item(href='login') Login
            a.collapse-item(href='register') Register
            a.collapse-item(href='forgot-password') Forgot Password
            div.collapse-divider
            h6.collapse-header Other Pages:
            a.collapse-item(href='404') 404 Page
            a.collapse-item(href='blank') Blank

      li.nav-item
        a.nav-link(href='charts')
          i.fas.fa-fw.fa-chart-area
            span Charts

      li.nav-item
        a.nav-link(href='tables')
          i.fas.fa-fw.fa-table
            span Tables

      hr.sidebar-divider.d-none.d-md-block

      div.text-center.d-none.d-none.d-md-block
        button.rounded-circle.border-0#sidebarToggle

    div#content-wrapper.d-fex.flex-column

      div#content

        nav.navbar.navbar-expand.navbar-light.bg-white.topbar.mb-4.static-top.shadow

          button#sidebarToggleTop.btn.btn-link.d-.mod-none.rounded-circle.mr-3
            i.fa.fa-bars

          form.d-none.d-sm-inline.form-inline.mr-auto.ml-md-3.my-2.my-md-2.my-md-0.mw-100.navbar-search
            div.input-group
              input.from-control.bg-light.border-0.small(type="text" placeholder="Search for..." aria-label="Search" aria-describedby="basic-addon2")
              div.input-group-append
                button.btn.btn-primary(type="button")
                  i.fas.fa-search.fa-sm

          ul.navbar-nav.ml-auto //Zeile 169 von index.html




      
      
      

      



